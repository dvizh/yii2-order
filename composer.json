{
    "name": "dvizh/yii2-order",
    "description": "Admin panel and widgets for applicaion orders",
    "keywords": ["yii2", "yii 2", "application", "order", "module", "dvizh"],
    "homepage": "https://github.com/dvizh/yii2-order",
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "support": {
        "source": "https://github.com/dvizh/yii2-order"
    },
    "authors": [
        {
            "name": "Ilya Pistol",
            "email": "dvizh@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "*",
        "yiisoft/yii2-bootstrap": "*",
        "2amigos/yii2-grid-view-library": "*",
        "kartik-v/yii2-grid": "*",
        "kartik-v/yii2-export": "@dev",
        "kartik-v/yii2-mpdf": "dev-master",
        "nex/yii2-datepicker": "*",
        "dvizh/dic": "*"
    },
    "config": {
        "process-timeout": 1800
    },
    "autoload": {
        "psr-4": {
            "dvizh\\order\\": "src/"
        }
    },
    "extra": {
        "bootstrap": "dvizh\\order\\Bootstrap"
    }
}
